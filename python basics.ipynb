number = int(input("Enter a number: "))
reverse = 0

while number > 0:
    digit = number % 10
    reverse = reverse * 10 + digit
    number = number // 10

print("Reversed number:", reverse)


scores = [10, 20, 4, 45, 99, 99]

highest = second = float('-inf')

for score in scores:
    if score > highest:
        second = highest
        highest = score
    elif score > second and score != highest:
        second = score

print("Second highest:", second)


